{
  "schemaVersion": 1,
  "id": "manymods",
  "version": "${version}",

  "name": "Many Mods",
  "description": "This is mod which combines many mods into a single JAR file.",
  "authors": [
    "H1ppyChick & Torphedo"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/manymods",
    "sources": "https://github.com/h1ppyChick/manyMods"
  },

  "license": "CC0-1.0",
  "icon": "assets/manymods/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": ["combined.ManyModsClient"],
    "modmenu": ["combined.gui.ConfigApiImpl"],
    "main": ["combined.ManyMods"],
    "preLaunch": ["combined.ManyModsPreLaunch"]
	},
  "jars": ${nestedJars},
  "mixins": ["manymods.mixins.json"],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.2"
  }
}
