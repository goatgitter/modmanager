{
  "schemaVersion": 1,
  "id": "modmenuext",
  "version": "${version}",

  "name": "Mod Menu Extension",
  "description": "This is mod which extends the Mod Menu mod to add ability to enable/disable mods in the mod menu.",
  "authors": [
    "H1ppyChick"
  ],
  "contact": {
    "homepage": "https://github.com/h1ppyChick/modmenuext",
    "sources": "https://github.com/h1ppyChick/modmenuext"
  },

  "license": "CC0-1.0",
  "icon": "assets/modmenuext/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": ["com.github.h1ppyChick.modmenuext.ModMenuExtClient"],
    "modmenu": ["com.github.h1ppyChick.modmenuext.gui.ConfigApiImpl"],
    "main": ["com.github.h1ppyChick.modmenuext.ModMenuExt"],
    "preLaunch": ["com.github.h1ppyChick.modmenuext.ModMenuExtPreLaunch"]
	},
  "jars": ${nestedJars},
  "mixins": ["modmenuext.mixins.json"],
  "depends": {
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
