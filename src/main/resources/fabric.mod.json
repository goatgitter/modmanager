{
  "schemaVersion": 1,
  "id": "modmanager",
  "version": "${version}",

  "name": "Mod Manager",
  "description": "This is mod which extends the Mod Menu mod to add ability to enable/disable mods in the mod menu.",
  "authors": [
    "H1ppyChick"
  ],
  "contact": {
    "homepage": "https://github.com/h1ppyChick/modmanager",
    "sources": "https://github.com/h1ppyChick/modmanager"
  },

  "license": "CC0-1.0",
  "icon": "assets/modmanager/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": ["com.github.h1ppyChick.modmanager.ModClient"],
    "modmenu": ["com.github.h1ppyChick.modmanager.gui.ConfigApiImpl"],
    "main": ["com.github.h1ppyChick.modmanager.ModManager"],
    "preLaunch": ["com.github.h1ppyChick.modmanager.ModPreLaunch"]
	},
  "jars": ${nestedJars},
  "mixins": ["modmanager.mixins.json"],
  "depends": {
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
